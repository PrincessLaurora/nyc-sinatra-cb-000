<h1>Edit Landmark</h1>

<form action="/landmarks/<%= @landmark.id %>" method="POST">
  <input type="hidden" name="_method" value="PATCH">

  <h3><label>Name</label></h3>
  <input type="text" name="landmark[name]" id="landmark_name" value="@landmark.name"></br>

  <h3><label>Year Completed</label></h3>
  <input type="text" name="landmark[year_completed]" id="landmark_year_completed" value = "@landmark.year_completed.to_s"></br>

  <input type="submit" value="Edit New Landmark">
</form>
